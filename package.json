{
  "name": "@meetalva/material-design",
  "description": "Components that implement Google's Material Design",
  "version": "1.1.6",
  "main": "lib/index.js",
  "alva": {
    "name": "Material UI",
    "image": "http://media.meetalva.io/library/material-design/image.png",
    "color": "#3F51B5",
    "analysis": "alva.json"
  },
  "files": [
    "lib",
    "alva.json"
  ],
  "scripts": {
    "analyze": "alva-analyze --in package.json --out alva.json --format json",
    "build": "tsc --skipLibCheck",
    "build:watch": "npm run build  -- -w --skipLibCheck",
    "clean": "rm -rf lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meetalva/material-design.git"
  },
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^3.5.1",
    "typescript": "^3.0.3",
    "@meetalva/analyzer-cli": "^1.0.1-alpha.4"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@meetalva/essentials": "^1.0.0",
    "@types/node": "^8.0.53",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "react": "^16.5.1",
    "react-dom": "^16.7.0",
    "tslib": "^1.7.0"
  },
  "homepage": "https://github.com/meetalva/material-design"
}
